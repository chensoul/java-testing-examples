{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "ignorePaths": [],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/spring-boot-example/src/test/java/.+\\.java$/"
      ],
      "matchStrings": [
        "\"(?<depName>.*?):(?<currentValue>.*?)\""
      ],
      "datasourceTemplate": "docker"
    }
  ],
  "npm": {
    "enabled": false
  }
}
